  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shawnrushefsky/awsuper/issues"
  },
  "homepage": "https://github.com/shawnrushefsky/awsuper#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.1.4",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.253.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
